{
	"name": "example", // コンテナ名
	"dockerComposeFile": "docker-compose.yml", // docker-composeファイルのパス
	"service": "app", // 使用するサービス名
	"workspaceFolder": "/app", // ワークスペースのパス

	// VScodeの設定
	"customizations": {
		"vscode": {
			"extensions": [
				"rust-lang.rust-analyzer",
				"tamasfe.even-better-toml",
				"vadimcn.vscode-lldb"
			]
		}

	},
	"postCreateCommand": "pre-commit install"
}
